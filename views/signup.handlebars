<div>
  <h2>Sign Up</h2>
  <form id="signup-form">
    <div>
      <label for="username-input-signup">Username</label>
      <input type="text" id="username-input-signup" />
    </div>
    <div>
      <label for="email-input-signup">Email</label>
      <input type="text" id="email-input-signup" />
    </div>
    <div>
      <label for="password-input-signup">Password</label>
      <input type="password" id="password-input-signup" />
    </div>
    <button type="submit" id="signup-btn">Signup!</button>
    <a href="/login">Login instead</a>
  </form>
</div>

<script src="js/signup.js"></script>
